<?xml version="1.0"?>
<!-- 1 BuildRegion and 1 BuildInst -->
<!-- 1 SourceFacility FrontEnd, 1 SinkFacility BackEnd, 1 BatchReactor Reactor -->

<simulation>
  <control>
    <duration>1200</duration>
    <startmonth>1</startmonth>
    <startyear>2000</startyear>
    <simstart>0</simstart>
    <decay>0</decay>
  </control>

  <commodity>
    <name>fresh_PWR_uo2</name>
  </commodity>

  <commodity>
    <name>spent_PWR_uo2</name>
  </commodity>

  <market>
    <name>fresh_PWR_uo2market</name>
    <mktcommodity>fresh_PWR_uo2</mktcommodity>
    <model>
      <NullMarket/>
    </model>
  </market>

  <market>
    <name>spent_PWR_uo2market</name>
    <mktcommodity>spent_PWR_uo2</mktcommodity>
    <model>
      <NullMarket/>
    </model>
  </market>

  <facility>
    <name>FrontEnd</name>
    <model>
      <SourceFacility>
        <output>
          <outcommodity>fresh_PWR_uo2</outcommodity>
          <output_capacity>1e9</output_capacity>
          <inventorysize>1e10</inventorysize>
          <recipe>fresh_PWR_uo2_recipe</recipe>
        </output>
      </SourceFacility>
    </model>
    <outcommodity>fresh_PWR_uo2</outcommodity>
  </facility>

  <facility>
    <name>Reactor</name>
    <lifetime>720</lifetime>
    <model>
      <BatchReactor>
        <fuel_input>
         <incommodity>fresh_PWR_uo2</incommodity>
         <inrecipe>fresh_PWR_uo2_recipe</inrecipe>
        </fuel_input>
        <fuel_output>
         <outcommodity>spent_PWR_uo2</outcommodity>
         <outrecipe>spent_PWR_uo2_recipe</outrecipe>
        </fuel_output>
        <cyclelength>12</cyclelength>
        <refueldelay>0</refueldelay>
        <incoreloading>9.9459e04</incoreloading>
        <outcoreloading>9.9459e04</outcoreloading>
        <batchespercore>5</batchespercore>
        <commodity_production>
          <commodity>lwr_power</commodity>
          <capacity>1000</capacity>
          <cost>1000</cost>
        </commodity_production>
      </BatchReactor>
    </model>
    <incommodity>fresh_PWR_uo2</incommodity>
    <outcommodity>spent_PWR_uo2</outcommodity>
  </facility>

  <facility>
    <name>BackEnd</name>
    <model>
      <SinkFacility>
        <input>
          <commodities>
            <incommodity>spent_PWR_uo2</incommodity>
          </commodities>
          <input_capacity>1e9</input_capacity>
          <inventorysize>1e10</inventorysize>
        </input>
      </SinkFacility>
    </model>
    <incommodity>spent_PWR_uo2</incommodity>
  </facility>

  <region>
    <name>oneRegion</name>
    <allowedfacility>FrontEnd</allowedfacility>
    <allowedfacility>Reactor</allowedfacility>
    <allowedfacility>BackEnd</allowedfacility>
    <model>
      <NullRegion/>
    </model>
    <institution>
      <name>oneInst</name>
      <availableprototype>FrontEnd</availableprototype>
      <availableprototype>Reactor</availableprototype>
      <availableprototype>BackEnd</availableprototype>
      <initialfacilitylist>
        <entry>
          <prototype>FrontEnd</prototype>
          <number>1</number>
        </entry>
        <entry>
          <prototype>Reactor</prototype>
          <number>1</number>
        </entry>
        <entry>
          <prototype>BackEnd</prototype>
          <number>1</number>
        </entry>
      </initialfacilitylist>
      <model>
        <NullInst/>
      </model>
    </institution>
  </region>

